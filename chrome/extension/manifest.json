{
    "name": "KeyFish",
    "description": "Local password management tool.",
    "version": "1.3",
    "manifest_version": 2,
    "icons": {
	"128": "icons/keyfish128.png",
	"32": "icons/keyfish32.png",
	"19": "icons/keyfish19.png"
    },
    "permissions": [
	"tabs",
	"clipboardWrite"
    ],
    "browser_action": {
	"default_icon": "icons/keyfish19.png",
	"default_title": "KeyFish",
	"default_popup": "main.html"
    },
    "incognito": "spanning",
    "offline_enabled": true,
    "commands": {
	"_execute_browser_action": {
	    "suggested_key": {
		"default": "Ctrl+Shift+K"
	    }
	}
    }
}
