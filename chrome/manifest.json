{
    "name": "KeyFish",
    "description": "Local password management tool.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
	"128": "icons/keyfish128.png",
	"32": "icons/keyfish32.png",
	"19": "icons/keyfish19.png"
    },
    "permissions": [
	"tabs",
	"http://*/*",
	"https://*/*",
	"clipboardWrite"
    ],
    "browser_action": {
	"default_icon": "icons/keyfish19.png",
	"default_title": "KeyFish",
	"default_popup": "main.html"
    },
    "incognito": "spanning",
    "offline_enabled": true
}
